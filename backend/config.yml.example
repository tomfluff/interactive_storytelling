openai:
  key: YOUR_OPENAI_KEY
  org: YOUR_OPENAI_ORG
  model: gpt-4-1106-preview
  fmodel: gpt-3.5-turbo-1106
  vmodel: gpt-4-vision-preview
  vgen: dall-e-3
  tts: tts-1
